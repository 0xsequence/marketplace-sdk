{
	"name": "@0xsequence/marketplace-sdk",
	"private": false,
	"version": "1.2.1",
	"type": "module",
	"sideEffects": [
		"**/*.css"
	],
	"scripts": {
		"build": "tsdown",
		"format": "biome format --write .",
		"format:check": "biome format .",
		"biome:check": "biome check . --write",
		"check": "tsc --noEmit",
		"lint": "biome lint --write .",
		"lint:check": "biome lint .",
		"barrel": "ctix build",
		"test": "vitest",
		"test:update": "vitest --update",
		"coverage": "vitest run --coverage",
		"eslint": "eslint .",
		"eslint:fix": "eslint --fix .",
		"changeset": "changeset",
		"version": "changeset version && node ../scripts/update-marketplace-version.js",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"test-storybook": "test-storybook",
		"tailwind": "node ./compile-tailwind.js"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"./react": {
			"types": "./dist/react/index.d.ts",
			"default": "./dist/react/index.js"
		},
		"./react/hooks": {
			"types": "./dist/react/hooks/index.d.ts",
			"default": "./dist/react/hooks/index.js"
		},
		"./react/queries": {
			"types": "./dist/react/queries/index.d.ts",
			"default": "./dist/react/queries/index.js"
		},
		"./react/ssr": {
			"types": "./dist/react/ssr/index.d.ts",
			"default": "./dist/react/ssr/index.js"
		},
		"./marketplace-logos": {
			"types": "./dist/react/ui/components/marketplace-logos/index.d.ts",
			"default": "./dist/react/ui/components/marketplace-logos/index.js"
		},
		"./styles/index.css": {
			"default": "./dist/styles/index.css"
		},
		"./styles/preset": {
			"default": "./dist/styles/index.css"
		}
	},
	"dependencies": {
		"0xtrails": "^0.9.5",
		"@0xsequence/api-client": "workspace:*",
		"@databeat/tracker": "^0.9.3",
		"@tailwindcss/cli": "^4.1.18",
		"@xstate/store": "catalog:",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"date-fns": "catalog:",
		"dnum": "catalog:",
		"path-to-regexp": "^8.3.0",
		"react-day-picker": "catalog:",
		"tailwind-merge": "^3.4.0"
	},
	"peerDependencies": {
		"0xsequence": "catalog:",
		"@0xsequence/api": "catalog:",
		"@0xsequence/checkout": "catalog:",
		"@0xsequence/connect": "catalog:",
		"@0xsequence/design-system": "catalog:",
		"@0xsequence/indexer": "catalog:",
		"@0xsequence/metadata": "catalog:",
		"@0xsequence/network": "catalog:",
		"@0xsequence/waas": "catalog:",
		"@google/model-viewer": "catalog:",
		"@tanstack/react-query": "catalog:",
		"nuqs": "catalog:",
		"react": "catalog:",
		"react-dom": "catalog:",
		"viem": "catalog:",
		"wagmi": "catalog:"
	},
	"peerDependenciesMeta": {
		"nuqs": {
			"optional": true
		}
	},
	"devDependencies": {
		"@biomejs/biome": "catalog:",
		"@changesets/cli": "catalog:",
		"@eslint/js": "catalog:",
		"@storybook/addon-docs": "^10.1.10",
		"@storybook/addon-vitest": "^10.1.10",
		"@storybook/react-vite": "^10.1.10",
		"@storybook/test-runner": "^0.24.2",
		"@tailwindcss/postcss": "^4.1.18",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/react": "^16.3.1",
		"@types/react": "catalog:",
		"@types/react-dom": "catalog:",
		"@vitest/browser": "4.0.16",
		"@vitest/coverage-v8": "4.0.16",
		"eslint": "catalog:",
		"eslint-config-biome": "catalog:",
		"eslint-plugin-react": "catalog:",
		"eslint-plugin-react-hooks": "catalog:",
		"eslint-plugin-storybook": "catalog:",
		"globals": "catalog:",
		"happy-dom": "^20.0.11",
		"jsdom": "^27.3.0",
		"msw": "^2.12.4",
		"msw-storybook-addon": "^2.0.6",
		"playwright": "^1.57.0",
		"postcss": "^8.5.6",
		"prool": "^0.2.1",
		"storybook": "^10.1.10",
		"tailwindcss": "catalog:",
		"tsdown": "0.17.0-beta.6",
		"typescript": "catalog:",
		"typescript-eslint": "catalog:",
		"vite-tsconfig-paths": "catalog:",
		"vitest": "4.0.16"
	},
	"msw": {
		"workerDirectory": [
			"public"
		]
	}
}
