{
	"name": "@0xsequence/marketplace-sdk",
	"private": false,
	"version": "0.10.0",
	"type": "module",
	"sideEffects": [
		"**/*.css"
	],
	"scripts": {
		"build": "tsdown",
		"format": "biome format --write .",
		"format:check": "biome format .",
		"biome:check": "biome check . --write",
		"check": "tsc --noEmit",
		"lint": "biome lint --write .",
		"lint:check": "biome lint .",
		"barrel": "ctix build",
		"test": "vitest",
		"test:update": "vitest --update",
		"coverage": "vitest run --coverage",
		"eslint": "eslint .",
		"eslint:fix": "eslint --fix .",
		"changeset": "changeset",
		"version": "changeset version && node ../scripts/update-marketplace-version.js",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"test-storybook": "test-storybook"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"./react": {
			"types": "./dist/react/index.d.ts",
			"default": "./dist/react/index.js"
		},
		"./react/hooks": {
			"types": "./dist/react/hooks/index.d.ts",
			"default": "./dist/react/hooks/index.js"
		},
		"./react/queries": {
			"types": "./dist/react/queries/index.d.ts",
			"default": "./dist/react/queries/index.js"
		},
		"./react/ssr": {
			"types": "./dist/react/ssr/index.d.ts",
			"default": "./dist/react/ssr/index.js"
		},
		"./marketplace-logos": {
			"types": "./dist/react/ui/components/marketplace-logos/index.d.ts",
			"default": "./dist/react/ui/components/marketplace-logos/index.js"
		},
		"./react-day-picker/style.css": {
			"default": "./dist/react-day-picker.css"
		},
		"./styles/index.css": {
			"default": "./dist/index.css"
		},
		"./styles/preset": {
			"default": "./dist/index.css"
		}
	},
	"dependencies": {
		"@databeat/tracker": "^0.9.3",
		"@legendapp/state": "3.0.0-beta.31",
		"@tailwindcss/cli": "^4.1.11",
		"@xstate/store": "^3.7.0",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"date-fns": "^4.1.0",
		"dnum": "^2.15.0",
		"nuqs": "^2.4.3",
		"react-day-picker": "^9.7.0",
		"tailwind-merge": "^3.3.1"
	},
	"peerDependencies": {
		"0xsequence": "^2.3.20",
		"@0xsequence/api": "^2.3.20",
		"@0xsequence/checkout": "^5.3.6",
		"@0xsequence/connect": "^5.3.6",
		"@0xsequence/design-system": "^2.1.15",
		"@0xsequence/indexer": "^2.3.20",
		"@0xsequence/metadata": "^2.3.20",
		"@0xsequence/network": "^2.3.20",
		"@google/model-viewer": "^4.1.0",
		"@tanstack/react-query": "^5.81.2",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"viem": "^2.31.4",
		"wagmi": "^2.15.6"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.0.5",
		"@changesets/cli": "^2.29.5",
		"@eslint/js": "^9.29.0",
		"@storybook/addon-docs": "^9.0.14",
		"@storybook/addon-vitest": "^9.0.14",
		"@storybook/react-vite": "^9.0.14",
		"@storybook/test": "^8.6.14",
		"@storybook/test-runner": "^0.23.0",
		"@tailwindcss/postcss": "^4.1.11",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/react": "^16.3.0",
		"@types/react": "^19.1.8",
		"@types/react-dom": "^19.1.6",
		"@vitest/browser": "3.2.4",
		"@vitest/coverage-v8": "^3.2.4",
		"eslint": "^9.29.0",
		"eslint-config-biome": "^1.9.4",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-storybook": "^9.0.14",
		"globals": "^16.2.0",
		"happy-dom": "^18.0.1",
		"jsdom": "^26.1.0",
		"msw": "^2.10.2",
		"msw-storybook-addon": "^2.0.5",
		"playwright": "^1.53.1",
		"postcss": "^8.5.6",
		"prool": "^0.0.24",
		"rollup-preserve-directives": "^1.1.3",
		"storybook": "^9.0.14",
		"tailwindcss": "^4.1.11",
		"tsdown": "^0.12.9",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.35.0",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^3.2.4"
	},
	"msw": {
		"workerDirectory": [
			"public"
		]
	}
}
