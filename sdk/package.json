{
	"name": "@0xsequence/marketplace-sdk",
	"private": false,
	"version": "0.9.0",
	"type": "module",
	"sideEffects": [
		"**/*.css"
	],
	"scripts": {
		"build": "tsdown",
		"format": "biome format --write .",
		"format:check": "biome format .",
		"biome:check": "biome check . --write",
		"check": "tsc --noEmit",
		"lint": "biome lint --write .",
		"lint:check": "biome lint .",
		"barrel": "ctix build",
		"test": "vitest",
		"test:update": "vitest --update",
		"coverage": "vitest run --coverage",
		"eslint": "eslint .",
		"eslint:fix": "eslint --fix .",
		"changeset": "changeset",
		"version": "changeset version && node ../scripts/update-marketplace-version.js",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"test-storybook": "test-storybook",
		"test-storybook:ci": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"pnpm build-storybook --quiet && npx http-server storybook-static --port 6006 --silent\" \"wait-on tcp:6006 && pnpm test-storybook\""
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"./react": {
			"types": "./dist/react/index.d.ts",
			"default": "./dist/react/index.js"
		},
		"./react/hooks": {
			"types": "./dist/react/hooks/index.d.ts",
			"default": "./dist/react/hooks/index.js"
		},
		"./react/queries": {
			"types": "./dist/react/queries/index.d.ts",
			"default": "./dist/react/queries/index.js"
		},
		"./react/ssr": {
			"types": "./dist/react/ssr/index.d.ts",
			"default": "./dist/react/ssr/index.js"
		},
		"./marketplace-logos": {
			"types": "./dist/react/ui/components/marketplace-logos/index.d.ts",
			"default": "./dist/react/ui/components/marketplace-logos/index.js"
		},
		"./react-day-picker/style.css": {
			"default": "./dist/react-day-picker.css"
		},
		"./styles/index.css": {
			"default": "./dist/index.css"
		},
		"./styles/preset": {
			"default": "./dist/index.css"
		}
	},
	"dependencies": {
		"@databeat/tracker": "^0.9.3",
		"@legendapp/state": "catalog:",
		"@tailwindcss/cli": "0.0.0-insiders.aa85931",
		"@xstate/store": "catalog:",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"date-fns": "catalog:",
		"dnum": "catalog:",
		"nuqs": "catalog:",
		"react-day-picker": "catalog:",
		"tailwind-merge": "^3.3.1"
	},
	"peerDependencies": {
		"0xsequence": "catalog:",
		"@0xsequence/api": "catalog:",
		"@0xsequence/checkout": "catalog:",
		"@0xsequence/connect": "catalog:",
		"@0xsequence/design-system": "catalog:",
		"@0xsequence/indexer": "catalog:",
		"@0xsequence/metadata": "catalog:",
		"@0xsequence/network": "catalog:",
		"@google/model-viewer": "catalog:",
		"@tanstack/react-query": "catalog:",
		"react": "catalog:",
		"react-dom": "catalog:",
		"viem": "catalog:",
		"wagmi": "catalog:"
	},
	"devDependencies": {
		"@biomejs/biome": "catalog:",
		"@changesets/cli": "catalog:",
		"@eslint/js": "^9.29.0",
		"@storybook/addon-a11y": "^9.0.13",
		"@storybook/addon-docs": "^9.0.13",
		"@storybook/addon-onboarding": "^9.0.13",
		"@storybook/react": "^9.0.13",
		"@storybook/react-vite": "^9.0.13",
		"@storybook/test-runner": "^0.23.0",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/react": "^16.3.0",
		"@testing-library/user-event": "^14.6.1",
		"@types/react": "catalog:",
		"@types/react-dom": "catalog:",
		"@vitest/coverage-v8": "^3.2.4",
		"axe-playwright": "^2.1.0",
		"eslint": "^9.29.0",
		"eslint-config-biome": "^1.9.4",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-storybook": "^9.0.13",
		"globals": "^16.2.0",
		"happy-dom": "^18.0.1",
		"jsdom": "^26.1.0",
		"msw": "^2.10.2",
		"msw-storybook-addon": "^2.0.5",
		"playwright": "^1.53.1",
		"postcss": "^8.5.6",
		"prool": "^0.0.24",
		"rollup-preserve-directives": "^1.1.3",
		"storybook": "^9.0.13",
		"tailwindcss": "catalog:",
		"tsdown": "^0.12.9",
		"typescript": "catalog:",
		"typescript-eslint": "^8.35.0",
		"vite-tsconfig-paths": "catalog:",
		"vitest": "^3.2.4"
	}
}
