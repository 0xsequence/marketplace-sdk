{
	"private": true,
	"type": "module",
	"scripts": {
		"build": "pnpm run --r --filter \"./sdk\" build",
		"deps": "pnpx taze -r",
		"format": "biome format --write .",
		"format:check": "biome format .",
		"biome-check": "biome check . --unsafe --write",
		"check": "pnpm run --recursive check",
		"lint": "biome lint --write .",
		"lint:check": "biome lint .",
		"dev:next": "pnpm --filter sequence-marketplace-sdk-playground-next dev",
		"dev:react": "pnpm --filter sequence-marketplace-sdk-playground-react-vite dev",
		"dev:alternative-wallets": "pnpm --filter sequence-marketplace-sdk-playground-alternative-wallets dev",
		"eslint": "pnpm run --r --filter \"./sdk\" eslint",
		"eslint:fix": "pnpm run --r --filter \"./sdk\" eslint:fix",
		"preinstall": "pnpx only-allow pnpm",
		"postinstall": "bash ./post-install.sh",
		"knip": "knip",
		"test": "pnpm run --r --filter \"./sdk\" test",
		"coverage": "pnpm run --r --filter \"./sdk\" coverage",
		"tailwind": "pnpm --filter \"./sdk\" tailwind",
		"resolve-deps-and-build": "node scripts/resolve-deps-and-build.cjs"
	},
	"packageManager": "pnpm@10.23.0",
	"engines": {
		"node": "24.x"
	},
	"pnpm": {
		"overrides": {
			"@0xsequence/marketplace-sdk": "workspace:*",
			"@biomejs/biome": "2.2.7",
			"@dynamic-labs/ethereum": "4.40.0",
			"@dynamic-labs/rpc-providers": "4.40.0",
			"@dynamic-labs/sdk-api-core": "0.0.820",
			"@dynamic-labs/sdk-react-core": "4.40.0",
			"@dynamic-labs/types": "4.33.0",
			"@dynamic-labs/utils": "4.40.0",
			"@dynamic-labs/wagmi-connector": "4.40.0",
			"@dynamic-labs/wallet-book": "4.40.0",
			"@dynamic-labs/wallet-connector-core": "4.40.0",
			"@tanstack/react-query": "5.80.10",
			"@types/node": "24.10.0",
			"@types/react": "19.1.0",
			"@types/react-dom": "19.1.0",
			"@vitejs/plugin-react": "5.1.0",
			"knip": "5.60.1",
			"lefthook": "2.0.0",
			"msw": "2.12.0",
			"nuqs": "2.7.0",
			"react-router": "7.9.0",
			"tailwind-merge": "3.3.0",
			"typescript-eslint": "8.46.0",
			"viem": "2.38.2",
			"wagmi": "2.18.0",
			"vite": "7.1.4"
		},
		"onlyBuiltDependencies": [
			"@tailwindcss/oxide",
			"msw",
			"oxc-resolver"
		]
	},
	"devDependencies": {
		"@biomejs/biome": "catalog:",
		"@types/node": "^24.10.1",
		"knip": "^5.70.1",
		"lefthook": "^2.0.4",
		"typescript": "catalog:"
	},
	"prepare": "lefthook install",
	"dependencies": {
		"@babel/core": "^7.28.5"
	}
}
