{
	"name": "@0xsequence/marketplace-sdk",
	"private": false,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"build": "tsc -b && tsup",
		"lint": "biome lint .",
		"lint:fix": "biome lint --write .",
		"format": "biome format --write .",
		"format:check": "biome format .",
		"barrel": "ctix build"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"./react": {
			"types": "./dist/react/index.d.ts",
			"default": "./dist/react/index.js"
		},
		"./react/hooks": {
			"types": "./dist/react/hooks/index.d.ts",
			"default": "./dist/react/hooks/index.js"
		},
		"./react/ssr": {
			"types": "./dist/react/ssr/index.d.ts",
			"default": "./dist/react/ssr/index.js"
		}
	},
	"dependencies": {
		"@legendapp/state": "^3.0.0-beta.0",
		"@0xsequence/api": "^2.0.8",
		"@0xsequence/design-system": "^1.7.6",
		"@0xsequence/indexer": "^2.0.8",
		"@0xsequence/kit": "4.0.4",
		"@0xsequence/kit-checkout": "4.0.4",
		"@0xsequence/metadata": "^2.0.8",
		"@0xsequence/network": "^2.0.8",
		"@tanstack/react-query": "^5.56.2",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"viem": "^2.21.12",
		"wagmi": "^2.12.13"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.2",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"ctix": "^2.6.4",
		"tsup": "^8.3.0",
		"typescript": "^5.5.3"
	}
}
