{
  "name": "@0xsequence/marketplace-sdk",
  "private": false,
  "version": "2.0.1",
  "type": "module",
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "build": "tsdown",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "biome:check": "biome check . --write",
    "check": "tsc --noEmit",
    "lint": "biome lint --write .",
    "lint:check": "biome lint .",
    "barrel": "ctix build",
    "test": "vitest",
    "test:update": "vitest --update",
    "coverage": "vitest run --coverage",
    "eslint": "eslint .",
    "eslint:fix": "eslint --fix .",
    "changeset": "changeset",
    "version": "changeset version && node ../scripts/update-marketplace-version.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-storybook": "test-storybook",
    "tailwind": "node ./compile-tailwind.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "default": "./dist/react/index.js"
    },
    "./react/hooks": {
      "types": "./dist/react/hooks/index.d.ts",
      "default": "./dist/react/hooks/index.js"
    },
    "./react/queries": {
      "types": "./dist/react/queries/index.d.ts",
      "default": "./dist/react/queries/index.js"
    },
    "./react/ssr": {
      "types": "./dist/react/ssr/index.d.ts",
      "default": "./dist/react/ssr/index.js"
    },
    "./marketplace-logos": {
      "types": "./dist/react/ui/components/marketplace-logos/index.d.ts",
      "default": "./dist/react/ui/components/marketplace-logos/index.js"
    },
    "./styles/index.css": {
      "default": "./dist/styles/index.css"
    },
    "./styles/preset": {
      "default": "./dist/styles/index.css"
    }
  },
  "dependencies": {
    "0xtrails": "0.9.6",
    "@databeat/tracker": "0.9.3",
    "@tailwindcss/cli": "4.1.18",
    "@xstate/store": "3.14.1",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "date-fns": "4.1.0",
    "dnum": "2.17.0",
    "path-to-regexp": "8.3.0",
    "react-day-picker": "9.13.0",
    "tailwind-merge": "3.4.0"
  },
  "peerDependencies": {
    "0xsequence": "2.3.39",
    "@0xsequence/api": "2.3.39",
    "@0xsequence/checkout": "5.4.8",
    "@0xsequence/connect": "5.4.8",
    "@0xsequence/design-system": "3.2.0",
    "@0xsequence/indexer": "2.3.39",
    "@0xsequence/metadata": "2.3.39",
    "@0xsequence/network": "2.3.39",
    "@0xsequence/waas": "2.3.39",
    "@google/model-viewer": "4.1.0",
    "@tanstack/react-query": "5.90.16",
    "nuqs": "2.8.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "viem": "2.44.4",
    "wagmi": "2.18.0"
  },
  "peerDependenciesMeta": {
    "nuqs": {
      "optional": true
    }
  },
  "devDependencies": {
    "@0xsequence/api-client": "link:../api",
    "@biomejs/biome": "2.3.11",
    "@changesets/cli": "2.29.8",
    "@eslint/js": "9.39.2",
    "@storybook/addon-docs": "10.1.11",
    "@storybook/addon-vitest": "10.1.11",
    "@storybook/react-vite": "10.1.11",
    "@storybook/test-runner": "0.24.2",
    "@tailwindcss/postcss": "4.1.18",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.1",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@vitest/browser": "4.0.16",
    "@vitest/coverage-v8": "4.0.16",
    "eslint": "9.39.2",
    "eslint-config-biome": "2.1.3",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-storybook": "10.1.11",
    "globals": "16.5.0",
    "happy-dom": "20.0.11",
    "jsdom": "27.4.0",
    "msw": "2.12.7",
    "msw-storybook-addon": "2.0.6",
    "playwright": "1.57.0",
    "postcss": "8.5.6",
    "prool": "0.2.2",
    "storybook": "10.1.11",
    "tailwindcss": "4.1.18",
    "tsdown": "0.17.0-beta.6",
    "typescript": "5.9.3",
    "typescript-eslint": "8.51.0",
    "vite-tsconfig-paths": "6.0.3",
    "vitest": "4.0.16"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
