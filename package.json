{
  "name": "@0xsequence/marketplace-sdk",
  "private": false,
  "version": "1.2.0",
  "type": "module",
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "build": "tsdown",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "biome:check": "biome check . --write",
    "check": "tsc --noEmit",
    "lint": "biome lint --write .",
    "lint:check": "biome lint .",
    "barrel": "ctix build",
    "test": "vitest",
    "test:update": "vitest --update",
    "coverage": "vitest run --coverage",
    "eslint": "eslint .",
    "eslint:fix": "eslint --fix .",
    "changeset": "changeset",
    "version": "changeset version && node ../scripts/update-marketplace-version.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-storybook": "test-storybook",
    "tailwind": "node ./compile-tailwind.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "default": "./dist/react/index.js"
    },
    "./react/hooks": {
      "types": "./dist/react/hooks/index.d.ts",
      "default": "./dist/react/hooks/index.js"
    },
    "./react/queries": {
      "types": "./dist/react/queries/index.d.ts",
      "default": "./dist/react/queries/index.js"
    },
    "./react/ssr": {
      "types": "./dist/react/ssr/index.d.ts",
      "default": "./dist/react/ssr/index.js"
    },
    "./marketplace-logos": {
      "types": "./dist/react/ui/components/marketplace-logos/index.d.ts",
      "default": "./dist/react/ui/components/marketplace-logos/index.js"
    },
    "./styles/index.css": {
      "default": "./dist/styles/index.css"
    },
    "./styles/preset": {
      "default": "./dist/styles/index.css"
    }
  },
  "dependencies": {
    "@databeat/tracker": "0.9.3",
    "@legendapp/state": "3.0.0-beta.32",
    "@tailwindcss/cli": "4.1.17",
    "@xstate/store": "3.11.2",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "date-fns": "4.1.0",
    "dnum": "2.15.0",
    "react-day-picker": "9.11.1",
    "tailwind-merge": "3.4.0",
    "xtrails": "0.1.13"
  },
  "peerDependencies": {
    "0xsequence": "2.3.33",
    "@0xsequence/api": "2.3.33",
    "@0xsequence/checkout": "5.4.1",
    "@0xsequence/connect": "5.4.1",
    "@0xsequence/design-system": "2.2.5",
    "@0xsequence/indexer": "2.3.33",
    "@0xsequence/metadata": "2.3.33",
    "@0xsequence/waas": "2.3.33",
    "@0xsequence/network": "2.3.33",
    "@google/model-viewer": "4.1.0",
    "@tanstack/react-query": "5.90.10",
    "nuqs": "2.7.3",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "viem": "2.39.0",
    "wagmi": "2.19.4"
  },
  "peerDependenciesMeta": {
    "nuqs": {
      "optional": true
    }
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.6",
    "@changesets/cli": "2.29.7",
    "@eslint/js": "9.39.1",
    "@storybook/addon-docs": "9.1.16",
    "@storybook/addon-vitest": "9.1.16",
    "@storybook/react-vite": "9.1.16",
    "@storybook/test-runner": "0.23.0",
    "@tailwindcss/postcss": "4.1.17",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@types/react": "19.2.5",
    "@types/react-dom": "19.2.3",
    "@vitest/browser": "4.0.0-beta.12",
    "@vitest/coverage-v8": "4.0.0-beta.12",
    "eslint": "9.39.1",
    "eslint-config-biome": "2.1.3",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-storybook": "9.1.16",
    "globals": "16.5.0",
    "happy-dom": "18.0.1",
    "jsdom": "26.1.0",
    "msw": "2.12.2",
    "msw-storybook-addon": "2.0.6",
    "playwright": "1.56.1",
    "postcss": "8.5.6",
    "prool": "0.0.24",
    "rollup-preserve-directives": "1.1.3",
    "storybook": "9.1.16",
    "tailwindcss": "4.1.17",
    "tsdown": "0.14.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.4",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "4.0.0-beta.12"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
